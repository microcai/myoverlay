[Unit]
Description=squid service

[Service]
StandardOutput=syslog
ExecStart=/usr/sbin/squid -N 
ExecReload=/usr/sbin/squid -k reconfigure
ExecStop=/usr/sbin/squid -k shutdown
KillSignal=SIGINT

